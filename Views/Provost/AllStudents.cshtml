@using HallManagementSystem.Models;
@model IEnumerable<Registration>

@{
    ViewBag.Title = "AllStudents";
    Layout = "~/Views/Shared/_AllStudents.cshtml";
}
<div class="col-md-3">

</div>
<div class="col-md-6">
    <h2 style="text-align:center;">Students List</h2>
    @foreach (var item in Model)
{

    <p><a href="@Href("~/Provost/studentDetail/" + item.stuid)"><span class="glyphicon glyphicon-arrow-right" style="color:black; font-size:larger; font-family:'Book Antiqua';">@item.name</span></a></p>

}

</div>
<div class="col-md-3">

</div>
