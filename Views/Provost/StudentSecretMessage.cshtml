@using HallManagementSystem.Models;
@model IEnumerable<StudentProvostSecretMsg>
@{
    ViewBag.Title = "StudentSecretMessage";
    Layout = "~/Views/Shared/_StudentSecretMessage.cshtml";
}

<div class="col-md-3">
</div>
<div class="col-md-6">
    <h2 style="text-align:center;">Message List</h2>
    @foreach (var item in Model)
    {

        <p><a href="@Href("~/Provost/AllSecretMessages/" + item.id)"> <span class="glyphicon glyphicon-arrow-right" style="color:black; font-size:larger; font-family:'Book Antiqua';">@item.id</span></a></p>

    }

</div>
<div class="col-md-3">

</div>
