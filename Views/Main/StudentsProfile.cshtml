@using HallManagementSystem.Models;
@model IEnumerable<Registration>
@{
    ViewBag.Title = "StudentsProfile";
    Layout = "~/Views/Shared/_StudentsProfile.cshtml";
}

<div class="col-md-4">

</div>
<div class="col-md-4">
    <h2 style="text-align:center;">Student  Details</h2>
    @{
        foreach (var item in Model)
        {
            <img src="~/Content/@item.image" class="img-thumbnail" alt="Cinque Terre" width="150" height="110">

            <p>Name: @item.name</p>
            <p>Faculty: @item.faculty</p>
            <p>Student Id: @item.stuid</p>
            <p>Session: @item.session</p>
            <p>Home Town: @item.district</p>
            <p>Contact Number: @item.contact</p>
            <p>Email Address: @item.email</p>
        }
    }

</div>
<div class="col-md-4">

</div>

